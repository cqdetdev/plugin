{
  "name": "@dragonfly/proto",
  "version": "0.1.0",
  "type": "module",
  "main": "src/generated/plugin.js",
  "exports": {
    "./*": {
      "types": "./src/generated/*.ts",
      "import": "./src/generated/*.js"
    },
    ".": {
      "types": "./src/generated/plugin.ts",
      "import": "./src/generated/plugin.js"
    }
  },
  "scripts": {
    "build": "tsc --module ES2022 --target ES2022 --moduleResolution node --skipLibCheck --outDir src/generated src/generated/*.ts"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "^2.2.2"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}